<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="mysqlDao" class="com.douye.dao.UserDaoImpl.MysqlDaoImpl">
    </bean>
    <bean id="oracleDao" class="com.douye.dao.UserDaoImpl.OracleDaoImpl"></bean>

    <bean id="userService" class="com.douye.service.UserServiceImpl.UserServiceImpl">
        <property name="userDao" ref="oracleDao"></property>
    </bean>
</beans>